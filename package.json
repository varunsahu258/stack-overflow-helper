{
  "name": "stack-overflow-helper",
  "displayName": "Stack Overflow Helper",
  "icon": "images/icon.png",
  "description": "Fetches Stack Overflow solutions for errors in VS Code",
  "version": "1.0.0",
  "publisher": "VarunSahu258",
  "repository": {
    "type": "git",
    "url": "https://github.com/varunsahu258/stack-overflow-helper"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
  "onLanguage:javascript",
  "onLanguage:python",
  "onLanguage:java",
  "onLanguage:csharp",
  "onLanguage:cpp",
  "onLanguage:html",
  "onLanguage:css",
  "onLanguage:typescript",
  "onLanguage:ruby",
  "onLanguage:php",
  "onLanguage:go",
  "onLanguage:swift",
  "onLanguage:kotlin",
  "onLanguage:rust",
  "onLanguage:dart",
  "onLanguage:json",
  "onLanguage:yaml",
  "onLanguage:xml",
  "onLanguage:markdown",
  "onLanguage:latex",
  "onLanguage:sql",
  "onLanguage:powershell",
  "onLanguage:bash",
  "onLanguage:scala",
  "onLanguage:groovy",
  "onLanguage:typescriptreact",
  "onLanguage:javascriptreact",
  "onLanguage:vue",
  "onLanguage:handlebars",
  "onLanguage:less",
  "onLanguage:sass",
  "onLanguage:scss",
  "onLanguage:graphql",
  "onLanguage:plaintext",
  "onLanguage:sh",
  "onLanguage:ini"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "stack-overflow-helper.searchErrors",
        "title": "Search Stack Overflow for Errors"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "echo \"No tests yet\""
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.14",
    "@types/vscode": "^1.75.0",
    "assert": "^2.1.0",
    "mocha": "^11.1.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^1.8.4"
  },
  "homepage": "https://github.com/varunsahu258/stack-overflow-extension",
  "bugs": {
    "url": "https://github.com/varunsahu258/stack-overflow-extension/issues"
  },
  "tags": [
    "Stack Overflow",
    "error handling",
    "solution finder",
    "VS Code",
    "programming"
  ]
}
